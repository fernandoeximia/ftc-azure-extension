{
    "manifestVersion": 1,
    "id": "ftc-azure-extension",
    "publisher": "fernandoNeiva",
    "version": "1.0.9",
    "name": "ftc-azure-extension",
    "description": "Extensão para criação de user stories com IA",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "my-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "My Hub",
                "uri": "my-hub.html"
            }
        },
        {
            "id": "ai-user-stories-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Grupo para geração de User Stories com IA",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Gerador de User Stories com IA",
                "uri": "work-item-group.html",
                "height": 200
            }
        }
    ],
    "files": [
        {
            "path": "my-hub.html",
            "addressable": true
        },
        {
            "path": "work-item-group.html",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}